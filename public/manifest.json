{
  "manifest_version": 2,
  "name": "XP Newtab",
  "author": "Etesam Ansari",
  "version": "1.0.3",
  "description": "A new tab extension that is in the style of Windows XP.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "unlimitedStorage"
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}
